{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "appearanceTools": true,
        "color": {
            "defaultPalette": false,
            "palette": [
                { 
                    "slug": "rh-green", 
                    "color": "rgb(228, 231, 225)", 
                    "name": "Rose Hill Green" 
                },
                {
                    "slug": "rh-bright-green",
                    "color": "rgb(196, 224, 209)",
                    "name": "Rose Hill Bright Green"
                },
                {
                    "slug": "rh-sea-green",
                    "color": "rgb(197, 211, 201)",
                    "name": "Rose Hill Sea Green"
                },
                {
                    "slug": "rh-darker-green",
                    "color": "rgb(216, 219, 213)",
                    "name": "Rose Hill Darker Green"
                },
                {
                    "slug": "rh-yellow",
                    "color": "rgb(232, 220, 173)",
                    "name": "Rose Hill Yellow"
                },
                {
                    "slug": "rh-blue",
                    "color": "rgb(188, 215, 216)",
                    "name": "Rose Hill Blue"
                },
                {
                    "slug": "rh-beige",
                    "color": "rgb(211, 209, 187)",
                    "name": "Rose Hill Beige"
                },
                {
                    "slug": "rh-gray",
                    "color": "rgb(222, 223, 221)",
                    "name": "Rose Hill Gray"
                },
                {
                    "slug": "rh-black",
                    "color": "rgb(27, 30, 29)",
                    "name": "Rose Hill Black"
                },
                {
                    "slug": "rh-white",
                    "color": "rgb(255, 255, 255)",
                    "name": "Rose Hill White"
                }
            ],
            "duotone": [
                {
                    "colors": [ "rgb(255, 255, 255)", "rgb(255, 255, 255)" ],
					"slug": "rh-pure-white",
					"name": "RH Pure White"
                },
                {
                    "colors": [ "rgb(27, 30, 29)", "rgb(255, 255, 255)" ],
					"slug": "rh-black-white",
					"name": "RH Black & White"
                },
                {
                    "colors": [ "rgb(76,76,76)", "rgb(238,238,238)" ],
					"slug": "rh-grayscale",
					"name": "RH Grayscale"
                }
            ],
            "background": true,
            "text": true,
            "link": true,
            "custom": true,
            "defaultDuotone": false,
            "defaultGradients": false
        },
        "blocks": {
            "core/site-title": {
                "color": {
                    "defaultPalette": false,
                    "link": true
                },
                "typography": {
                    "fontFamilies": [
                        {
                            "fontFamily": "Kelson Sans, sans-serif",
                            "slug": "rh-kelson-sans",
                            "name": "RH Kelson Sans"
                        }
                    ]
                }
            },
            "core/navigation": {
                "color": {
                    "defaultPalette": false,
                    "link": true
                },
                "typography": {
                    "fontFamilies": [
                        {
                            "fontFamily": "Barlow Semi Condensed, sans-serif",
                            "slug": "rh-barlow",
                            "name": "RH Barlow"
                        }
                    ]
                }
            },
            "core/preformatted": {
                "typography": {
                    "fontSizes": [],
                    "customFontSize": false
                }
            },
            "core/column": {
                "border": {
                    "radius": true
                }
            },
            "core/query-pagination-previous": {
                "spacing": {
                    "margin": true,
                    "padding": true
                },
                "border": {
                    "radius": true
                }
            }
            ,
            "core/query-pagination-next": {
                "spacing": {
                    "margin": true,
                    "padding": true
                },
                "border": {
                    "radius": true
                }
            },
            "core/query-pagination-numbers": {
                "spacing": {
                    "margin": true,
                    "padding": true
                }
            }
        },
        "border": {
            "color": true,
            "radius": true,
            "style": true,
            "width": true
        },
        "typography": {
            "fontFamilies": [
                {
                    "fontFamily": "Anonymous Pro, monospace",
                    "slug": "rh-anonymous-pro",
                    "name": "RH Anonymous Pro"
                },
                {
                    "fontFamily": "Silkscreen, monospace",
                    "slug": "rh-silkscreen",
                    "name": "RH Silkscreen"
                },
                {
                    "fontFamily": "Kelson Sans",
                    "slug": "rh-kelson-sans",
                    "name": "RH Kelson Sans",
                    "fontFace": [
                        {
                            "fontFamily": "Kelson Sans",
							"fontWeight": "300",
							"src": [ "file:./assets/fonts/Kelson-Sans-Light.otf" ]
                        },
                        {
                            "fontFamily": "Kelson Sans",
							"fontWeight": "400",
							"src": [ "file:./assets/fonts/Kelson-Sans-Regular.otf" ]
                        },
                        {
                            "fontFamily": "Kelson Sans",
							"fontWeight": "700",
							"src": [ "file:./assets/fonts/Kelson-Sans-Bold.otf" ]
                        }
                    ]
                }
            ],
            "fontSizes": [
                {
                    "slug": "small",
                    "size": "0.75rem",
                    "name": "Small"
                },
                {
                    "slug": "medium",
                    "size": "1.25rem",
                    "name": "Medium"
                },
                {
                    "slug": "large",
                    "size": "2.25rem",
                    "name": "Large"
                },
                {
                    "slug": "x-large",
                    "size": "3.25rem",
                    "name": "Extra Large"
                },
                {
                    "slug": "gigantic",
                    "size": "3.75rem",
                    "name": "Gigantic"
                }
            ],
            "fontWeight": true,
            "fontStyle": true,
            "textTransform": true,
            "textDecoration": true,
            "lineHeight": true
        },
        "layout": {
            "contentSize": "85%"
        },
        "spacing": {
            "margin": true,
            "padding": true,
            "units": [
                "px", "rem", "vh", "vw", "%"
            ],
            "blockGap": true
        }
    },
    "styles": {
        "color": {
            "text": "var(--wp--preset--color--rh-black)",
            "background": "var(--wp--preset--color--rh-gray)"
        },
        "elements": {
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--rh-black)"
                },
                "typography": {
                    "textDecoration": "underline"
                }
            },
            "h1": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--rh-kelson-sans)",
                    "fontWeight": "400"
                }
            },
            "h2": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--rh-kelson-sans)",
                    "fontWeight": "400"
                }
            },
            "h3": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--rh-kelson-sans)",
                    "fontWeight": "400"
                }
            },
            "h4": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--rh-kelson-sans)",
                    "fontWeight": "400"
                }
            },
            "h5": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--rh-kelson-sans)",
                    "fontWeight": "400"
                }
            },
            "h6": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--rh-kelson-sans)",
                    "fontWeight": "400"
                }
            }
        },
        "blocks": {
            "core/site-title": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--rh-kelson-sans)",
                    "textTransform": "uppercase",
                    "fontWeight": "700"
                },
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                }
            },
            "core/navigation": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--rh-kelson-sans)",
                    "fontWeight": "700",
                    "letterSpacing": "1px",
                    "textTransform": "uppercase"
                }
            },
            "core/navigation-submenu": {
                "css": "& ul {padding: 5px; border: 0px!important} & li {text-transform: none; font-weight: 400}"
            },
            "core/query-pagination-previous": {
                "spacing": {
                    "padding": {
                        "top": "10px",
                        "bottom": "10px",
                        "left": "10px",
                        "right": "10px"
                    }
                }
            },
            "core/query-pagination-next": {
                "spacing": {
                    "padding": {
                        "top": "10px",
                        "bottom": "10px",
                        "left": "10px",
                        "right": "10px"
                    }
                }
            },
            "core/query-pagination-numbers": {
                "spacing": {
                    "padding": {
                        "top": "10px",
                        "bottom": "10px",
                        "left": "10px",
                        "right": "10px"
                    }
                }
            },
            "core/heading": {
                "typography": {
                    "letterSpacing": "1px"
                }
            },
            "core/post-title": {
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                }
            },
            "core/search": {
                "elements": {
                    "button": {
                        "spacing": {
                            "padding": {
                                "top": "0px",
                                "right": "0px",
                                "bottom": "0px",
                                "left": "0px"
                            }
                        },
                        "color": {
                            "text": "var(--wp--preset--color--rh-black)",
                            "background": "transparent"
                        }
                    }
                }
            },
            "core/list-item": {
                "typography": {
                    "lineHeight": "1.5rem"
                },
                "spacing": {
                    "padding": {
                        "top": "5px",
                        "bottom": "5px"
                    }
                }
            },
            "core/button": {
                "color": {
                    "text": "var(--wp--preset--color--rh-white)",
                    "background": "var(--wp--preset--color--rh-black)"
                },
                "border": {
                    "radius": "4px"
                }
            },
            "core/columns": {
                "spacing": {
                    "blockGap": "4rem"
                }
            },
            "core/pullquote": {
                
                "spacing": {
                    "padding": {
                        "top": "10px",
                        "bottom": "10px",
                        "left": "5px",
                        "right": "5px"
                    }
                },
                "typography": {
                    "fontSize": "20px"
                },
                "css": "& p {line-height: 1.5rem} & cite {font-size: 16px; font-weight: bold} & cite::before {content: '- '} & cite::after {content: ' -'}"
            },
            "core/details": {
                "typography": {
                    "lineHeight": "1.6rem"
                }
            },
            "core/paragraph": {
                "typography": {
                    "lineHeight": "1.5rem"
                }
            }
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--rh-anonymous-pro)",
            "fontSize": "16px",
            "fontStyle": "normal",
            "fontWeight": "normal",
            "lineHeight": "auto",
            "textDecoration": "none",
            "textTransform": "none"
        },
        "spacing": {
            "margin": {
                "top": "0px",
                "bottom": "0px",
                "left": "0px",
                "right": "0px"
            },
            "padding": {
                "top": "0px",
                "bottom": "0px",
                "left": "0px",
                "right": "0px"
            }
        }
    }
}